{
  "name": "mrb-demo-offline-application",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "directory": "https://github.com/SAPMarco/UI5-Offline-PPoC"
  },
  "description": "Best-practice starting point for building a freestyle app",
  "private": true,
  "scripts": {
    "start": "ui5 serve -o index.html",
    "serve": "ui5 serve",
    "lint": "eslint webapp",
    "build:ui": "ui5 build --clean-dest --dest approuter/webapp ",
    "build": "ui5 build self-contained --all --exclude-task=createDebugFiles --clean-dest --dest ./dist"
  },
  "dependencies": {
    "@openui5/sap.m": "^1.74.0",
    "@openui5/sap.ui.core": "^1.74.0",
    "@openui5/sap.ui.layout": "^1.74.0",
    "@openui5/themelib_sap_fiori_3": "^1.74.0",
    "shx": "^0.3.2"
  },
  "devDependencies": {
    "@ui5/cli": "^1.14.0",
    "eslint": "^6.8.0",
    "ui5-middleware-livereload": "^0.1.5"
  },
  "ui5": {
    "dependencies": [
      "@openui5/sap.m",
      "@openui5/sap.ui.core",
      "@openui5/sap.ui.layout",
      "@openui5/themelib_sap_fiori_3",
      "ui5-middleware-livereload"
    ]
  }
}
